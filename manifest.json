{
  "manifest_version": 2,
  "name": "Warframe Helper",
  "version": "1.0",
  "description": "This extension provides alerts and tools for Warframe.",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "activeTab",
	"notifications",
    "https://ajax.googleapis.com/"
  ]
}